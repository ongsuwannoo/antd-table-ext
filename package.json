{
  "name": "antd-table-ext",
  "version": "0.1.0",
  "description": "",
  "author": "ptrushin",
  "keywords": [
    "ui",
    "antd",
    "grid",
    "table",
    "resizable",
    "mavable",
    "hidable",
    "fixable"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ptrushin/antd-table-ext.git"
  },
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "dist/index.js",
  "private": false,
  "dependencies": {
    "clone-deep": "^4.0.1",
    "fast-deep-equal": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-drag-listview": "^0.2.1",
    "react-resizable": "^3.0.4",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:npm": "cross-env NODE_ENV=production && rimraf -rf dist && mkdir dist && npx babel src/lib --out-dir dist --copy-files",
    "prepublishOnly": "npm run build:npm"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "antd": "^4.23.6",
    "react-highlight-words": "^0.18.0"
  },
  "babel": {
    "presets": [
      "@babel/react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
